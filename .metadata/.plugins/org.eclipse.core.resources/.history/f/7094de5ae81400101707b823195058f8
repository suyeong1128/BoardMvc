package com.boardMvc.dao;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.jdbc.core.JdbcTemplate;
import org.springframework.stereotype.Repository;

import com.boardMvc.dto.UserDto;

@Repository
public class UserDaoImpl implements UserDao{
	@Autowired
	private JdbcTemplate jdbcTemplate;
	
	@Override
	public void insertUser(UserDto user) {
		String sql = "INSERT INTO custom_user(name, birth, nickName, email, phone, password) VALUES(?, ?, ?, ?, ?, ?)";
		jdbcTemplate.update(sql, user.getName(), user.getBirth(), user.getNickName(), user.getEmail(), user.getPhone(), user.getPassword());
	}
}
