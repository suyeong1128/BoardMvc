package com.boardMvc.dao;
import com.boardMvc.service.serviceImpl.MainServiceImple;
import org.springframework.stereotype.Repository;

import com.boardMvc.dto.BoardDto;

@Repository
public class BoardDaoImpl {

    private final MainServiceImple mainServiceImple;

    BoardDaoImpl(MainServiceImple mainServiceImple) {
        this.mainServiceImple = mainServiceImple;
    }
	public void insertBoard(BoardDto board) {
		String sql = "INSERT INTO board(nickName, title, content, createdAt) VALUES(?, ?, ?, ?)";
		jdbcTemplate.update(sql, board.getNickName(), board.getTitle(), board.getContent(), board.getCreatedAt());
	}
}
