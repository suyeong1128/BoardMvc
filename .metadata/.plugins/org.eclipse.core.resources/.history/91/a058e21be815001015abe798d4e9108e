	package com.boardMvc.service.serviceImpl;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

import com.boardMvc.dao.BoardDao;
import com.boardMvc.dto.BoardDto;
import com.boardMvc.mapper.MainMapper;
import com.boardMvc.service.MainService;


@Service
public class MainServiceImple implements MainService, BoardService {

	@Autowired
	MainMapper mainMapper;
	
	@Autowired
	private BoardDao boardDao;
	
	@Override
	public String main() {
		
		return mainMapper.main();
	}

	public void insertBoard(BoardDto board) {
		boardDao.insertBoard(board);
	}

}
